          <field name="inherit_id" ref="entity_sms.esms_history_form_view" />
          <field name="arch" type="xml">
            <group position="inside">
              <field name="cost" />
            </group>
          </field>
      </record>

      <record model="ir.ui.view" id="suvit_history_list_view">
